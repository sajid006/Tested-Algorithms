
#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define pb push_back
#define filein freopen("in.txt","r",stdin)
#define fileout freopen("out.txt","w",stdout)
#define all(v) v.begin(),v.end()
#define ff first
#define ss second
#define mod 1000000007
#define IOS ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define pll pair<ll,ll>

vector<ll>BIT;

ll getNext(ll index)
{
    return index + (index & -index);
}

ll getParent(ll index)
{
    return index - (index & -index);
}

void updateBIT(ll val,ll index)
{
    while(index < BIT.size())
    {
        BIT[index]+=val;
        index=getNext(index);
    }
}

ll getSum(ll index)
{
    index++;
    ll sum = 0;
    while(index > 0)
    {
        sum+=BIT[index];
        index=getParent(index);
    }
    return sum;
}

void createtree(vector<ll>input)
{
    ll len=input.size();
    vector<ll>ex(len+1,0);
    BIT=ex;
    for(ll i=1;i<=len;i++)
    {
        updateBIT(input[i-1],i);
    }
}

int main()
{

    ll n,i,j,x;
    vector<ll>input;
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>x;
        input.pb(x);
    }
    createtree(input);
    cout<<"Created"<<endl;
    ll q,a,b;
    cin>>q;
    while(q--)
    {
        cin>>a>>b;
        if(a==0)cout<<getSum(b)<<endl;
        else cout<<getSum(b)-getSum(a-1)<<endl;
    }
    return 0;
}
